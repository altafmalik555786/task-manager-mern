{
  "name": "task-manager",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": "14.21.3"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.19.5",
    "axios": "^0.19.2",
    "bootstrap": "4.3.1",
    "clean": "^4.0.2",
    "express": "^4.17.1",
    "history": "4.9.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "perfect-scrollbar": "1.4.0",
    "query-string": "^6.13.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-notification-alert": "0.0.12",
    "react-notifications": "^1.7.2",
    "react-redux": "^7.2.1",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "react-spinners": "^0.9.0",
    "react-switch": "^6.0.0",
    "reactstrap": "8.0.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "use-lodash-debounce": "^1.2.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css",
    "minify-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/paper-dashboard.scss src/assets/css/paper-dashboard.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "eqeqeq": "off",
      "jsx-a11y/alt-text": "off",
      "no-unused-vars": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "no-unreachable": "off",
      "import/no-anonymous-default-export": "off",
      "no-dupe-keys": "off",
      "no-lone-blocks": "off",
      "array-callback-return": "off"
    }
  },
  "optionalDependencies": {
    "@types/googlemaps": "3.36.2",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.8.19",
    "ajv": "6.10.0",
    "jquery": "3.4.1",
    "typescript": "3.4.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
